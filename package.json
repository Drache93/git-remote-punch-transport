{
  "name": "git-remote-punch",
  "version": "0.1.0",
  "description": "Git over holepunch remote helper.",
  "pear": {
    "name": "git-remote-punch",
    "type": "terminal"
  },
  "main": "runtime.js",
  "bin": {
    "git-remote-punch": "index.js"
  },
  "scripts": {
    "test": "brittle test/*.js",
    "fix": "standard --fix",
    "schema": "node schemas/local.js && node schemas/remote.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "brittle": "^3.18.0",
    "hyperdht": "^6.21.0",
    "standard": "^17.1.2",
    "test-tmp": "^1.4.0"
  },
  "imports": {
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "os": {
      "bare": "bare-os",
      "default": "os"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    },
    "process": {
      "bare": "bare-process",
      "default": "process"
    },
    "child_process": {
      "bare": "bare-subprocess",
      "default": "child_process"
    },
    "tty": {
      "bare": "bare-tty",
      "default": "tty"
    }
  },
  "dependencies": {
    "buffer": "npm:bare-buffer",
    "readline": "npm:bare-readline",
    "b4a": "^1.6.7",
    "bare-buffer": "^3.3.0",
    "bare-fs": "^4.1.6",
    "bare-os": "^3.6.1",
    "bare-path": "^3.0.0",
    "bare-process": "^4.2.1",
    "bare-readline": "^1.0.9",
    "bare-string-decoder": "^1.0.0",
    "bare-subprocess": "^5.1.0",
    "bare-tty": "^5.0.2",
    "compact-encoding": "^2.16.1",
    "compact-encoding-net": "^1.2.0",
    "compact-encoding-struct": "^1.3.0",
    "corestore": "^7.4.5",
    "hyperbee": "^2.24.3",
    "hyperblobs": "^2.8.0",
    "hypercore": "^11.11.1",
    "hyperdb": "^4.16.0",
    "hyperschema": "^1.13.0",
    "hyperswarm": "^4.12.1",
    "isomorphic-git": "^1.33.0",
    "pear-inspect": "^1.2.5",
    "pear-pipe": "^1.0.1",
    "pear-run": "^1.0.0",
    "ready-resource": "^1.1.2"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/git-remote-punch-transport.git"
  },
  "bugs": {
    "url": "https://github.com/holepunchto/git-remote-punch-transport/issues"
  },
  "homepage": "https://github.com/holepunchto/git-remote-punch-transport#readme"
}
